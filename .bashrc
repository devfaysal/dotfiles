#PHPunit
alias t="vendor/bin/phpunit --filter"
alias phpunit="vendor/bin/phpunit"

#PHPStan
alias phpstan="vendor/bin/phpstan analyse"

#Laravel
alias vapor="vendor/bin/vapor"

#Symfony
export PATH="$HOME/.symfony/bin:$PATH"

#git
alias gs="git status"
alias ga="git add"
alias gcm="git commit -m"
alias gpm="git push origin master"

#Expose
alias expose="~/.config/composer/vendor/beyondcode/expose/builds/expose"

#php7.4 composer
alias php7.4composer="/usr/bin/php7.4 /usr/local/bin/composer"


alias showsecond="gsettings set org.gnome.desktop.interface clock-show-seconds true"
alias hidesecond="gsettings set org.gnome.desktop.interface clock-show-seconds false"
